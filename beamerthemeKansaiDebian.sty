%% [takahashi method] %%%
\newcommand{\takahashi}[2][12]{
  {%
    \setbeamertemplate{headline}{}
    \setbeamertemplate{footline}{}
    \begin{frame}
      \centering
      \bfseries \fontsize{#1pt}{#1pt}\selectfont #2
    \end{frame}
  }
}

